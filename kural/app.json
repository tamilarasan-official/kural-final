{
  "expo": {
    "name": "kural",
    "slug": "kural",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "kural",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "AIzaSyCiwZbwy5fgQQ-0E6zFGrdSPWQGh1XVFPQ"
      }
    },
    "android": {
      "package": "com.kuralapp.kural",
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCiwZbwy5fgQQ-0E6zFGrdSPWQGh1XVFPQ"
        }
      }
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-web-browser",
      [
        "expo-build-properties",
        {
          "android": {
            "manifestPlaceholders": {
              "merge_strategy": "always_use_first"
            }
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "GOOGLE_MAPS_API_KEY": "AIzaSyCiwZbwy5fgQQ-0E6zFGrdSPWQGh1XVFPQ",
      "eas": {
        "projectId": "d77a475b-9d3c-4dc4-80d2-052a8d56a56b"
      }
    }
  }
}
